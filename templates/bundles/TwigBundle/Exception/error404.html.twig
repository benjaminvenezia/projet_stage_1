{% extends "base.html.twig" %}

{% block title %}
    Page non trouvée
{% endblock %}

{% block body %}

    <div class="container container__error404">
        <div class="row">
            <div class="col mx-auto text-center">
                 <h1>{{ status_code }}</h1>
                 <h2> {{ exception.message }}</p>
                <img src="{{ asset('/build/img/errors/notfound.png') }}" alt="knight around a table">
                <p>Les chevaliers de la table ronde revinrent les mains vides ce jour-là.</p>
            </div>
        </div>
    </div>

   
    
{% endblock %}